{
    "name": "phalconhosting/phmaster",
    "description": "Phalcon Hosting - Master server",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Stephen Hoogendijk",
            "email": "stephen@phalconhosting.com"
        },
        {
            "name": "Soufiane Ghzal",
            "email": "souf@phalconhosting.com"
        },
        {
            "name": "Ivo Stefanov",
            "email": "ivo@phalconhosting.com"
        }
    ],
    "require": {
        "php": ">=5.3.6",
        "phalcon/incubator": "dev-master"
    },
    "repositories": [
        {
            "url": "https://github.com/phalcon/incubator",
            "type": "package",
            "package": {
                "name": "phalcon/incubator",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/phalcon/incubator",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": ["/Library/Phalcon"]
                }
            }
        }
    ],
    "autoload": {
        "psr-0" : {
            "PH"    :   "app/library",
            "Phalcon\\Test"    :   "app/library"

        }
    }
}